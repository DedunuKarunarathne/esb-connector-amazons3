                {
                  "type": "attribute",
                  "value": {
                    "name": "sseCustomerKeyMD5",
                    "displayName": "Server Side Encryption Customer Key MD5",
                    "inputType": "stringOrExpression",
                    "defaultValue": "",
                    "required": "false",
                    "helpTip": "The 128-bit MD5 digest of the encryption key according to RFC 1321."
