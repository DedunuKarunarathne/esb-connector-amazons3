              "name": "configRef",
              "displayName": "Amazon S3 Connection",
              "inputType": "connection",
              "allowedConnectionTypes": [
                "amazons3"
              ],
              "defaultType": "connection.amazons3",
              "defaultValue": "",
              "required": "true",
              "helpTip": "Connection to be used"
